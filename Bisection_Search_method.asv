function [a, b, iter, app_min, min_val] = Bisection_Search_method(f, a, b, nmax, eps, delta)
    if nargin < 4 || isempty(nmax)
        nmax = 10;
    end
    if nargin < 5 || isempty(eps)
        eps = 1e-5;
    end
    if nargin < 6 || isempty(delta)
        delta = (b-a)*0.001;
    end

    N = log2((b-a)/eps);

    x1 = a + (b-a)/2 - delta/2;
    x2 = a + (b-a)/2 + delta/2;

    fx1 = f(x1);
    fx2 = f(x2);

    for iter = 1: nmax
        if fx1 < fx2
    end


end